<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dheeraj.com.trafficpolice">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="dheeraj.com.trafficpolice.FeedsActivity"
            android:label="@string/app_name">
        </activity>

        <activity android:name="dheeraj.com.trafficpolice.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="dheeraj.com.trafficpolice.LoginRegisterChoose" />
        <activity android:name="dheeraj.com.trafficpolice.UserProfile" />
        <activity android:name="dheeraj.com.trafficpolice.SignIn" />
        <activity android:name="dheeraj.com.trafficpolice.SignUp"></activity>
        <activity
            android:name="dheeraj.com.trafficpolice.ProfileActivity"
            android:label="@string/app_name">
        </activity>


        <activity
            android:name="dheeraj.com.trafficpolice.CommentsActivity"
            android:label="@string/title_activity_comments" />
        <activity
            android:name="dheeraj.com.trafficpolice.NewPostActivity"
            android:label="@string/title_activity_new_post" />
        <activity
            android:name="dheeraj.com.trafficpolice.UserDetailActivity"
            android:label="@string/title_activity_user_detail" />
        <activity
            android:name="dheeraj.com.trafficpolice.ParkingSearchResult"
            android:label="Parking Search Result" />
        <activity android:name="dheeraj.com.trafficpolice.BaseActivity"></activity>
        <service
            android:name="dheeraj.com.trafficpolice.GeoFenceService"
            android:enabled="true" />
    </application>

</manifest>
